<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anush.hackillinoisapp" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GpsActivity"
            android:label="@string/title_activity_gps" >
        </activity>
        <activity
            android:name=".LocationHolderActivity"
            android:label="@string/title_activity_location_holder" >
        </activity>
        <activity
            android:name=".DescriptiveViewActivity"
            android:label="@string/title_activity_descriptive_view" >
        </activity>
    </application>

</manifest>
